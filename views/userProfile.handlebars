<div class="container user-profile">
    <form action="POST">
        <div class="row">
            <div class="col-md-4">
                <div class="profile-img">
                    <img src="http://www.psnleaderboard.com/images/avatars/J0010.png" alt="">
                    <div class="file btn btn-lg btn-primary">
                        Change Photo
                        <input type="file" name="file" />
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="profile-head">
                    <h5>{{full_name}} </h5>
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab"
                                aria-controls="home" aria-selected="true">Orders</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " id="profile-tab" data-toggle="tab" href="#profile" role="tab"
                                aria-controls="profile" aria-selected="false">Account</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-md-2">
                <input type="submit" class="profile-edit-btn" name="btnAddMore" value="Edit Profile" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="profile-settings">
                    <p>Settings</p>
                    <a href="">Account</a><br />
                    <a href="">Current Order</a><br />
                    <a href="">Order History</a><br />
                    <a href="">Payment</a><br />
                </div>
            </div>
            <div class="col-md-8">
                <div class="tab-content profile-tab" id="myTabContent">
                    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                        {{#each ordersToInsert_final }}
                        <div class="row">
                            <div class="col-md-6">
                                <label>Order ID </label>
                            </div>
                            <div class="col-md-6">
                                <label>{{order_id}}</label>
                            </div>
                        </div>



                        <div class="row">
                            <div class="col-md-6">
                                <label>Items</label>
                            </div>
                            <div class="col-md-6">
                                <label>{{food_name}}</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Quantity</label>
                            </div>
                            <div class="col-md-6">
                                <label>{{quantity}}</label>
                            </div>
                        </div>
                        <hr>
                        {{/each}}
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <label>Grand_Total </label>
                        </div>
                        <div class="col-md-6">
                            <label>${{grand_total}}</label>
                        </div>
                    </div>



                    <div class="tab-pane fade" id="profile" role="tabpanel1" aria-labelledby="profile-tab">
                        <div class="row">
                            <div class="col-md-6">
                                <label>Name</label>
                            </div>
                            <div class="col-md-6">
                                <label>{{full_name}}</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Password</label>
                            </div>
                            <div class="col-md-6">
                                <label>*********</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Email</label>
                            </div>
                            <div class="col-md-6">
                                <label>{{email}}</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Phone</label>
                            </div>
                            <div class="col-md-6">
                                <label>9435 8762</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Address</label>
                            </div>
                            <div class="col-md-6">
                                <label>{{address}}</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>